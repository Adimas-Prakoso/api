{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "views/**",
                    "src/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/views/(.*)",
            "dest": "/views/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js"
        }
    ]
}